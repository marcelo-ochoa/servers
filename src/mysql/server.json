{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "io.github.marcelo-ochoa/mysql",
  "description": "MCP server for interacting with MySQL databases",
  "repository": {
    "url": "https://github.com/marcelo-ochoa/servers",
    "source": "github",
    "subfolder": "src/mysql"
  },
  "version": "1.0.4",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "@marcelo-ochoa/server-mysql",
      "version": "1.0.4",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "description": "MySQL user",
          "isRequired": true,
          "format": "string",
          "isSecret": true,
          "name": "MYSQL_USER"
        },
        {
          "description": "MySQL password",
          "isRequired": true,
          "format": "string",
          "isSecret": true,
          "name": "MYSQL_PASSWORD"
        }
      ]
    }
  ]
}