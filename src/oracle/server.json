{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "io.github.marcelo-ochoa/oracle",
  "description": "MCP server for interacting with Oracle databases",
  "repository": {
    "url": "https://github.com/marcelo-ochoa/servers",
    "source": "github",
    "subfolder": "src/oracle"
  },
  "version": "1.0.1",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "@marcelo-ochoa/server-oracle",
      "version": "1.0.1",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "description": "Oracle user name",
          "isRequired": true,
          "format": "string",
          "isSecret": false,
          "name": "ORACLE_USER"
        },
        {
          "description": "Oracle password",
          "isRequired": true,
          "format": "string",
          "isSecret": true,
          "name": "ORACLE_PASSWORD"
        }
      ]
    }
  ]
}